SINGLETON(x_) = CONS(x_ NIL);
COUNT(x_ CONS(x_ y_)) = SUCC(COUNT(x_ y_));
COUNT(x_ CONS(z_ y_)) = COUNT(x_ y_);
COUNT(x_ NIL) = DIGIT(0);
TAIL(NIL) = NIL;
TAIL(CONS(x_ y_)) = y_;
SUCC(DIGIT(0)) = DIGIT(1);
SUCC(DIGIT(1)) = DIGIT(2);
SUCC(DIGIT(2)) = DIGIT(3);
SUCC(DIGIT(3)) = DIGIT(4);
SUCC(DIGIT(4)) = DIGIT(5);
SUCC(DIGIT(5)) = DIGIT(6);
SUCC(DIGIT(6)) = DIGIT(7);
SUCC(DIGIT(7)) = DIGIT(8);
SUCC(DIGIT(8)) = DIGIT(9);
